{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@10.7.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "checks-all": "npm-run-all --parallel test type-check lint:*",
    "checks-all-ci": "npm-run-all --parallel type-check lint:*",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "test-watch": "vitest",
    "test": "vitest run",
    "test-coverage": "vitest run --coverage",
    "lint:styles": "stylelint **/*.css",
    "lint:scripts": "eslint --max-warnings=0 \"**/*.{ts?(x),?(c)js}\"",
    "lint-fix": "pnpm run lint:scripts --fix",
    "format": "prettier --write ./**/*.{js,ts,tsx,css,html}"
  },
  "dependencies": {
    "@adobe/react-spectrum": "^3.39.0",
    "@internationalized/date": "^3.5.2",
    "@tanstack/react-query": "^5.66.7",
    "@tanstack/react-query-devtools": "^5.66.7",
    "framer-motion": "^12.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "3.1.1",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.0",
    "stylelint": "^16.18.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-prettier": "^5.0.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0",
    "vitest": "^3.1.1"
  }
}
